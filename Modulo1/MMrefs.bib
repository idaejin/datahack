@techreport{SchabenbergerXX,
	Abstract = {In spatially arranged experiments and observational studies, inference
	about treatment effects,
	
	environmental conditions and covariate effects that determine the
	large-scale variation is focal point of
	
	the investigation, whereas the pattern and strength of spatial dependence
	is modeled primarily to allow
	
	efficient estimation of the parameters representing the large-scale
	variation. We present an approach to
	
	the estimation of large-scale trends in situations where spatial autocorrelation
	exists among the
	
	observations. A possible application is to designed experiments with
	a spatial dimension such as
	
	agricultural field experiments. A treatment structure is superimposed
	upon a spatial random process. It is
	
	proposed to estimate the parameters of the mean function as roots
	of quasi-likelihood type estimating
	
	functions and the spatial correlation parameters by composite likelihood
	based on pairwise differences of
	
	studentized residuals. The mean function of the response is parameterized
	in terms of linked linear
	
	predictors which enables the application to non-Gaussian data, provided
	the studentized residuals exhibit
	
	continuity. A comparative analysis of two agricultural field trials
	is presented along with a simulation
	
	study based on Gaussian random fields.},
	Author = {Schabenberger, O. and Kong, F. and Gregoire, T. G.},
	Owner = {Dae-Jin Lee},
	Timestamp = {2006.01.03},
	Title = {Generalized Linear Models for Spatial Data: A Composite Likelihood Approach},
